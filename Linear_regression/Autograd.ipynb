   "source": [
    "# z = 2w^2 + 5 \n",
    "y = w**2\n",
    "z = 2*y + 5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "7f654b68-fc73-4159-ad55-fc05de8e7053",
   "metadata": {},
   "outputs": [],
   "source": [
    "# z를 w에 관해서 미분\n",
    "z.backward()"
   ]
  },
